   "source": [
    "# relation between independent values and renewal\n",
    "# this shows that category on postive scale indicate renewal tendency and negative scale show non renewal tendency\n",
    "data_tab1 = pd.crosstab(data.perc_premium_paid_by_cash_credit_binned,data.renewal)/\\\n",
    "                        pd.crosstab(data.perc_premium_paid_by_cash_credit_binned,data.renewal).sum()\n",
    "data_tab1.columns = ['non_renewal','renewal']\n",
    "data_tab1['woe'] = np.log2(data_tab1.renewal/data_tab1.non_renewal)\n",
    "data_tab1['iv'] = data_tab1['woe']*(data_tab1.renewal-data_tab1.non_renewal)\n",
    "data_tab1 = data_tab1[data_tab1.iv>=0.02]\n",
    "data_tab1['woe'].plot(kind='bar')\n",
